<p id="notice"><%= notice %></p>

<h1>Listing Billboard Dates</h1>

<table>
  <thead>
    <tr>
      <th>Day</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @billboard_dates.each do |billboard_date| %>
      <tr>
        <td><%= billboard_date.day %></td>
        <td><%= link_to 'Show', billboard_date %></td>
        <td><%= link_to 'Edit', edit_billboard_date_path(billboard_date) %></td>
        <td><%= link_to 'Destroy', billboard_date, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Billboard date', new_billboard_date_path %>
